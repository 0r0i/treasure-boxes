schedule:
  minutes_interval>: 5

+query-monitoring:
    docker:
        image: "digdag/digdag-python:3.7.3-stretch"
    py>: query-monitoring.monitoring
    _env:
        td_apikey: ${secret:td.apikey}
        td_endpoint: https://api.treasuredata.com
        td_database: your_database
        td_table: monitoring
