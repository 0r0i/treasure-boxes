schedule:
  minutes_interval>: 5

+query-monitoring:
    docker:
        image: "digdag/digdag-python:3.7.3-stretch"
    py>: query-monitoring.monitoring
    _env:
        td_apikey: ${secret:td.apikey}
        td_endpoint: ${secret:td.endpoint}
        td_database: ${secret:td.database}
        td_table: ${secret:td.table}
